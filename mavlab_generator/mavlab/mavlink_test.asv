clear all;
clc;

echotcpip('on',14552);
t = tcpclient('localhost',14552);
parser = mavlink_parser();

msgSend = msg_attitude(1,2,3,4,5,6,7);
bufferSend = msgSend.pack().encode();

write(t, bufferSend);

while t.getBytesAvailable() > 0
    
    c = read(t,1);
    packetRec = parser.parseChar(uint8(c));
    if ~isempty(packetRec)
        msgRec = packetRec.unpack();
        if msgRec.msgid == 30
            msgRec
        end
        
    end
    
end

echotcpip('off');